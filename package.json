{
  "name": "jupyterlab-python-bytecode",
  "version": "0.6.0",
  "description": "JupyterLab extension to inspect Python Bytecode",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jtpio/jupyterlab-python-bytecode",
  "bugs": {
    "url": "https://github.com/jtpio/jupyterlab-python-bytecode/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jeremy Tuloup",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jtpio/jupyterlab-python-bytecode.git"
  },
  "scripts": {
    "build": "tsc",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepare": "npm run clean && npm run build",
    "test": "npm run test:chrome",
    "test:chrome": "python tests/run-test.py --browsers=ChromeHeadless karma.conf.js",
    "test:debug": "python tests/run-test.py --browsers=ChromeHeadless --singleRun=false --debug=true karma.conf.js",
    "test:firefox": "python tests/run-test.py --browsers=Firefox karma.conf.js"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0-rc.0",
    "@jupyterlab/apputils": "^1.0.0-rc.0",
    "@jupyterlab/coreutils": "^3.0.0-rc.0",
    "@jupyterlab/docmanager": "^1.0.0-rc.0",
    "@jupyterlab/filebrowser": "^1.0.0-rc.0",
    "@jupyterlab/fileeditor": "^1.0.0-rc.0",
    "@jupyterlab/services": "^4.0.0-rc.0",
    "lodash": "^4.17.11",
    "react": "~16.8.4",
    "react-dom": "~16.8.4",
    "react-syntax-highlighter": "^10.2.1"
  },
  "devDependencies": {
    "@jupyterlab/testutils": "^1.0.0-rc.0",
    "@types/chai": "~4.0.10",
    "@types/lodash": "~4.14.120",
    "@types/mocha": "^5.2.0",
    "@types/react": "~16.8.4",
    "@types/react-dom": "~16.8.4",
    "@types/react-syntax-highlighter": "^10.2.1",
    "chai": "~4.1.2",
    "css-loader": "^3.0.0",
    "es6-promise": "~4.1.1",
    "file-loader": "~1.1.11",
    "husky": "^1.0.0",
    "json-loader": "^0.5.7",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-typescript": "^4.1.0",
    "karma-webpack": "~3.0.5",
    "lint-staged": "^8.1.5",
    "mocha": "^5.1.1",
    "prettier": "^1.17.0",
    "raw-loader": "~0.5.1",
    "rimraf": "^2.6.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "~0.21.0",
    "thread-loader": "^1.1.5",
    "ts-loader": "~5.0.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.5.2",
    "url-loader": "~1.0.1",
    "webpack": "^4.32.2"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "resolutions": {
    "@types/react": "16.4.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
